<Window x:Class="IPCWarehouseApplication.TimerDelay"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IPCWarehouseApplication"
        mc:Ignorable="d"
        Title="Timer Delay" Height="150" Width="400" Icon="Timer.png" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label x:Name="lblTimerDelay" Content="Timer Delay (Mins) : " HorizontalAlignment="Left" Margin="25,20,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="cboTimerDelay" HorizontalAlignment="Left" Margin="150,20,0,0" VerticalAlignment="Top" Width="145" MaxDropDownHeight="200"/>
        <Button x:Name="cmdSave" Content="Save" HorizontalAlignment="Left" Height="29" Margin="55,65,0,0" VerticalAlignment="Top" Width="80" Click="cmdSave_Click"/>
        <Button x:Name="cmdClose" Content="Close" HorizontalAlignment="Left" Height="29" Margin="180,65,0,0" VerticalAlignment="Top" Width="80" Click="cmdCancel_Click" />
    </Grid>
</Window>
