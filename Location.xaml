<Window x:Class="IPCWarehouseApplication.Location"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IPCWarehouseApplication"
        mc:Ignorable="d"
        Title="Location" Height="150" Width="340" WindowStartupLocation="CenterScreen" WindowStyle="ThreeDBorderWindow" Icon="warehouse-inventory-icon-33867.png">
    <Grid>
        <Label x:Name="lblLocation" Content="Location : " HorizontalAlignment="Left" Margin="25,20,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="cboLocation" HorizontalAlignment="Left" Margin="120,20,0,0" VerticalAlignment="Top" Width="175"/>
        <Button x:Name="cmdSave" Content="Save" HorizontalAlignment="Left" Height="29" Margin="55,65,0,0" VerticalAlignment="Top" Width="80" Click="cmdSave_Click"/>
        <Button x:Name="cmdClose" Content="Close" HorizontalAlignment="Left" Height="29" Margin="180,65,0,0" VerticalAlignment="Top" Width="80" Click="cmdCancel_Click" />
    </Grid>
</Window>
